                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Fruit"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Leave"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Rhizome"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Root"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Seed"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Stem"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
