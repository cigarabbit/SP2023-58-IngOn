        <ThaiIngredients:isUsedFor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Culinary"/>
        <ThaiIngredients:hasDescription>Offering a unique combination of spicy, citrusy, and slightly peppery flavors. It provides a sharper taste compared to ginger, with a tangy undertone that adds depth to Thai dishes.</ThaiIngredients:hasDescription>
        <ThaiIngredients:hasEnglishName>Ginger</ThaiIngredients:hasEnglishName>
        <ThaiIngredients:hasScientificName>Zingiber officinale</ThaiIngredients:hasScientificName>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Krachai -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Krachai">
        <ThaiIngredients:hasColor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#BrownAndYellow"/>
