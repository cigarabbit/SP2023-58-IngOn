    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Horapha -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Horapha">
        <ThaiIngredients:hasColor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Green"/>
        <ThaiIngredients:isUsedFor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#CulinaryAndHealthPotential"/>
        <ThaiIngredients:hasDescription>The taste of Horapha is both aromatic and peppery, with notes of clove and anise. It delivers a delightful combination of sweet and savory flavors, making it a distinctive and essential component of Thai cuisine.</ThaiIngredients:hasDescription>
        <ThaiIngredients:hasEnglishName>Thai sweet basil</ThaiIngredients:hasEnglishName>
        <ThaiIngredients:hasScientificName>Ocimum basilicum var. thyrsiflora</ThaiIngredients:hasScientificName>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Kaphrao -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Kaphrao">
        <ThaiIngredients:hasColor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Green"/>
        <ThaiIngredients:isUsedFor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#CulinaryAndHealthPotential"/>
        <ThaiIngredients:hasDescription>Holy Basil has an aromatic and peppery taste with notes of clove and anise. It offers a unique combination of sweet and savory flavors, making it a distinctive and essential component of Thai cuisine.</ThaiIngredients:hasDescription>
        <ThaiIngredients:hasEnglishName>Holy basil</ThaiIngredients:hasEnglishName>
        <ThaiIngredients:hasScientificName>Ocimum sanctum</ThaiIngredients:hasScientificName>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Kha -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Kha">
        <ThaiIngredients:hasColor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Green"/>
        <ThaiIngredients:isUsedFor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#CulinaryAndHealthPotential"/>
        <ThaiIngredients:hasDescription>The taste of Kha is bold and distinct, with a unique combination of spicy, citrusy, and slightly peppery flavors. It carries a sharper taste compared to ginger, with a tangy undertone that contributes depth to Thai dishes.</ThaiIngredients:hasDescription>
        <ThaiIngredients:hasEnglishName>Galangal</ThaiIngredients:hasEnglishName>
        <ThaiIngredients:hasScientificName>Alpinia galanga</ThaiIngredients:hasScientificName>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Khamin -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Khamin">
        <ThaiIngredients:hasColor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#OrangeAndYellow"/>
        <ThaiIngredients:isUsedFor rdf:resource="http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#CulinaryAndHealthPotential"/>
        <ThaiIngredients:hasDescription>Khamin boasts a warm and earthy flavor profile with subtle bitter undertones. Its taste is accompanied by hints of peppery notes, contributing depth and complexity to dishes.</ThaiIngredients:hasDescription>
        <ThaiIngredients:hasEnglishName>Turmeric</ThaiIngredients:hasEnglishName>
        <ThaiIngredients:hasScientificName>Curcuma longa</ThaiIngredients:hasScientificName>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/seniorProject/ontologies/2023/7/ThaiIngredients#Khing -->

